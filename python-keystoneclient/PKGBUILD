pkgname=python-keystoneclient
pkgver=0.1.3
pkgrel=1
pkgdesc="Keystone Identity Service Client"
url="http://launchpad.net/keystone"
license=('custom')
arch=('i686' 'x86_64')
depends=('python2')
conflicts=()
replaces=()
backup=()
source=("http://pypi.python.org/packages/source/${pkgname:0:1}/$pkgname/$pkgname-$pkgver.tar.gz")
md5sums=('da52d237390404e4d52f26354357fa8e')

build() {
	cd $srcdir/$pkgname-$pkgver
	python2 setup.py build
	python2 setup.py install --root=$pkgdir
	}
